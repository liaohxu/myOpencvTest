    "#需求：x轴 100 --> 200\n",
    "#       y轴 100 --> 300\n",
    "\n",
    "import cv2\n",
    "\n",
    "img = cv2.imread(\"..\\Images\\m5m.jpg\",1)\n",
    "imgInfo = img.shape\n",
    "print(imgInfo)\n",
    "\n",
    "# 通过切片的方式，实现 图片的剪切\n",
    "dst = img[300:400,400:600]\n",
    "print(dst.shape)\n",
    "\n",
    "cv2.imshow(\"myimage\",dst)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
